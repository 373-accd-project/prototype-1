<br/>
<%= form_tag("/launemp") do %>
  <h5>Pick Seasonal Adjustment: &nbsp; <%= select_tag(:seasonal_adjustment, options_for_select(@seasonal_adjustment_codes), multiple: true,  :style => "font-size:12px;") %><br/><br/></h5>
  <h5>Pick Areas: &nbsp; <%= select_tag(:area, options_for_select(@areas), multiple: true,  :style => "font-size:12px;") %><br/><br/></h5>
  <h5>Pick Measures: &nbsp; <%= select_tag(:measure, options_for_select(@measures), multiple: true,  :style => "font-size:12px;") %><br/><br/></h5>
  <h5>Input Start Year (YYYY): &nbsp;<%= text_field_tag(:start_year, Date.current.year - 11) %> </h5><br/>
  <h5>Input End Year (YYYY): &nbsp;<%= text_field_tag(:end_year, Date.current.year - 1) %> </h5><br/>
  <h5>Enter Series ID (optional): &nbsp;<%= text_field_tag(:series_id) %> </h5><br/>
  <%= submit_tag("Submit", multiple: true,  :style => "height:50px;width:100px;background-color:grey;color:white;", :class => "btn float-left" )%>
<% end %>

<p class = "float-left">&nbsp;&nbsp;&nbsp;</p>

<%= form_tag("/download_launemp_csv") do %>
  <%= submit_tag("Download", :style => "height:50px; width:200px;background-color:green;color:white;", :class => "btn float-left") %>
<% end %>
